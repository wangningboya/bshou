<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wy.bs.mapper.RoleMapper">

    <select id="getAll" resultType="cn.wy.bs.entity.Role">
        SELECT r.*
        FROM t_role r
        WHERE r.ISDELETE = 0
    </select>

</mapper>